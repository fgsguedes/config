[alias]
	l = log --date-order --date=iso --graph --full-history --all --pretty=format:'%x08%x09%C(red)%h %C(cyan)%ar%Creset %C(bold blue)%aN%C(reset)%C(bold yellow)%d %C(reset)%s'
	f = fetch --all
	st = status
	sl = stash list
	sa = stash apply
	sd = stash drop
[push]
	default = simple
[core]
	editor = emacs
	autocrlf = input
	name = Guedes
